{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*",
      "/js/*",
      "/icons/*"
    ]
  },
  "routes": [
    {
      "route": "/location/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/search",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}