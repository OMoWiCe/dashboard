{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/icons/*"]
  },
  "routes": [
    {
      "route": "/location/*",
      "rewrite": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/search",
      "rewrite": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}